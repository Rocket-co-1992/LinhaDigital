{
  "name": "easy-site-builder/backend",
  "description": "Backend for Easy Site Builder",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^6.0",
    "symfony/doctrine-bundle": "^2.7",
    "doctrine/orm": "^2.13",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "symfony/twig-bundle": "^6.0",
    "symfony/security-bundle": "^6.0",
    "symfony/mailer": "^6.0",
    "symfony/form": "^6.0",
    "symfony/validator": "^6.0",
    "symfony/asset": "^6.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "symfony/debug-bundle": "^6.0",
    "symfony/maker-bundle": "^1.48",
    "symfony/var-dumper": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.0.*"
    }
  }
}